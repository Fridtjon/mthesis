{
  "caliper": {
    "blockchain": "fabric",
    "command" : {
      "start": "sleep 1s",
      "end" : "sleep 1s"
    }
  },
  "fabric": {
    "cryptodir": "benchmark/hashcode_t4/crypto-config",
    "network": {
      "orderer": {
        "url": "grpc://158.39.75.116:7050",
        "mspid": "OrdererMSP",
        "msp": "benchmark/hashcode_t4/crypto-config/ordererOrganizations/example.com/msp/",
        "server-hostname": "orderer.example.com",
        "tls_cacerts": "benchmark/hashcode_t4/crypto-config/ordererOrganizations/example.com/orderers/orderer0.example.com/tls/ca.crt"
      },
      "org1": {
        "name": "peerOrg1",
        "mspid": "Org1MSP",
        "msp": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org1.example.com/msp/",
        "ca": {
          "url": "http://158.39.75.116:7054",
          "name": "ca.org1.example.com"
        },
        "peer1": {
          "requests": "grpc://158.37.63.164:7051",
          "events": "grpc://158.37.63.164:7053",
          "server-hostname": "peer0.org1.example.com",
          "tls_cacerts": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org1.example.com/peers/peer0.org1.example.com/tls/ca.crt"
        },        
        "peer2": {
          "requests": "grpc://158.37.63.165:7051",
          "events": "grpc://158.37.63.165:7053",
          "server-hostname": "peer1.org1.example.com",
          "tls_cacerts": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org1.example.com/peers/peer1.org1.example.com/tls/ca.crt"
        }
      },
      "org2": {
        "name": "peerOrg2",
        "mspid": "Org2MSP",
        "msp": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org2.example.com/msp/",
        "ca": {
          "url": "http://158.39.75.116:7054",
          "name": "ca.org2.example.com"
        },
        "peer1": {
          "requests": "grpc://158.37.63.175:7051",
          "events": "grpc://158.37.63.175:7053",
          "server-hostname": "peer0.org2.example.com",
          "tls_cacerts": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org2.example.com/peers/peer0.org2.example.com/tls/ca.crt"
        },        
        "peer2": {
          "requests": "grpc://158.37.63.184:7051",
          "events": "grpc://158.37.63.184:7053",
          "server-hostname": "peer1.org2.example.com",
          "tls_cacerts": "benchmark/hashcode_t4/crypto-config/peerOrganizations/org2.example.com/peers/peer1.org2.example.com/tls/ca.crt"
        }
      }
    },
    "channel": [
      {
        "name": "mychannel",
        "deployed": false,
        "config": "benchmark/hashcode_t4/channel.tx",
        "organizations": ["org1", "org2"]
      }
    ],
    "chaincodes": [
      {"id": "hashcode", "path": "contract/fabric/hashcode", "language":"golang", "version": "v1", "channel": "mychannel"}
    ],
    "endorsement-policy": {
      "identities": [
        {
          "role": {
            "name": "member",
            "mspId": "Org1MSP"
          }
        },
        {
          "role": {
            "name": "member",
            "mspId": "Org2MSP"
          }
        }
      ],
      "policy": { "2-of": [{"signed-by": 0}, {"signed-by": 1}]}
    },
    "context": {
      "smallOperations": "mychannel",
      "init": "mychannel",
      "open": "mychannel",
      "publish": "mychannel",
      "query": "mychannel"
    }
  },
  "info" : {
    "Version": "1.2.0",
    "Size": "2 Orgs with 1 Peers",
    "Orderer": "Kafka",
    "Distribution": "Multiple Hosts"
  }
}